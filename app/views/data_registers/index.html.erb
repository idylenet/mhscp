<h2>Listing data_registers</h2>

<table>
<thead>	
  <tr>
    <th>Name</th>
    <th>Register</th>
    <th>Writable</th>
    <th>Desc</th>
	<th colspan="3"></th>
  </tr>
</thead>

<% for data_register in @data_registers %>
  <tr>
    <td><%=h data_register.name %></td>
    <td><%=h data_register.register %></td>
    <td><%=h data_register.writable %></td>
    <td><%=h data_register.desc %></td>
    <td><%= link_to 'Show', data_register %></td>
    <td><%= link_to 'Edit', edit_data_register_path(data_register) %></td>
    <td><%= link_to 'Destroy', data_register, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New data_register', new_data_register_path %>
